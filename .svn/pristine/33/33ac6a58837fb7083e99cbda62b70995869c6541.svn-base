@charset "UTF-8";

#parking-box {
    border: 1px solid black;
    padding: 100px;
    height: 400px;
    margin: auto;
    display: flex;
    justify-content: center;
    align-items: center;
}

#parking-box > img {
    max-height: 120%; /* 이미지가 부모 요소의 높이를 넘지 않도록 */
    display: block; /* 이미지를 블록 요소로 표시하여 가운데 정렬에 도움 */
}